<div class="sg_image">
    <a href="javascript:void(0)" class="del" title="{{sgLang.delete}}"></a>
    <div class="img" style="background:url({{thumbPrefix}}{{data.sg_image}});"></div>
    <div class="name{{#ifCond data.sg_isactive "0"}} notactive{{/ifCond}}">{{data.sg_title}}</div>
    {{#ifCond data.sg_description ""}}<a href="javascript:void(0)" class="edit" title="{{sgLang.emptydesc}}"></a>{{/ifCond}}
</div>